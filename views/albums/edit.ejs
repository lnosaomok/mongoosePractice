<div class="container">
    <div class="card-panel">
        <div>
            <h1>Edit
                <%= foundAlbum.name %>'s Song</h1>
            <h3 class="center-align">Edit Song </h3>

        </div>
    </div>
    <div class="row">
        <form class="col s12" action="/albums/<%= foundAlbum._id %>/songs/<%= foundSong.id %>?_method=PUT" method="POST">

            <div class="row">
                <div class="input-field col s12">
                    <em>Song Name</em>
                    <input id="name" type="text" name="songName" value="<%= foundSong.songName %>" required>

                </div>

            </div>

            <button class="btn waves-effect waves-light" type="submit" name="action">Submit

            </button>
        </form>
    </div>