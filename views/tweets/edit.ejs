<div class="container">
    <div class="card-panel">
        <div>
            <h1>Edit
                <%= foundUser.name %>'s Tweet</h1>
            <h4 class="center-align">Edit Tweet </h4>

        </div>
    </div>
    <div class="row">
        <form class="col s12" action="/users/<%= foundUser.id %>/tweets/<%= foundTweet.id %>?_method=PUT" method="POST">

            <div class="row">
                <div class="input-field col s12">

                    <input id="name" type="text" name="tweetText" value="<%= foundTweet.tweetText %>" required>
                    <em>Tweet Text</em>
                </div>

            </div>
            <button class="btn waves-effect waves-light" type="submit" name="action">Submit

            </button>
        </form>
    </div>